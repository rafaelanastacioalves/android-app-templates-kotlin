<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools">

	<data>
		<import type="com.example.rafaelanastacioalves.moby.domain.entities.MainEntity"/>
		<import type="androidx.lifecycle.LiveData"/>
		<import type="com.example.rafaelanastacioalves.moby.domain.entities.Resource"/>
		<import type="java.util.List" />

		<variable
			name="viewmodel"
			type="com.example.rafaelanastacioalves.moby.ui.entitymainlisting.LiveDataMainEntityListViewModel" />
		<variable
			name="mainEntityListResult"
			type="LiveData&lt;Resource&lt;List&lt;MainEntity>>>"  />
	</data>

	<androidx.constraintlayout.widget.ConstraintLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context="com.example.rafaelanastacioalves.moby.ui.entitymainlisting.MainActivity">

		<androidx.recyclerview.widget.RecyclerView
			android:id="@+id/main_entity_list"
			android:layout_width="0dp"
			android:layout_height="0dp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />
	</androidx.constraintlayout.widget.ConstraintLayout>
</layout>
